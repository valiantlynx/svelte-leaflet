{
	"name": "svelte-blog",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"deploy-test": "pnpm format && pnpm lint && pnpm build && pnpm preview"
	},
	"devDependencies": {
		"@iconify/svelte": "^3.1.4",
		"@splidejs/svelte-splide": "^0.2.9",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.20.4",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"@valiantlynx/eslint-config-custom": "0.0.1",
		"@valiantlynx/general-config": "^0.0.19",
		"@valiantlynx/svelte-leaflet": "^0.0.6",
		"@valiantlynx/svelte-rich-text": "^0.0.4",
		"@valiantlynx/svelte-ui": "0.0.19",
		"@valiantlynx/tailwind-config": "0.0.9",
		"@valiantlynx/tsconfig": "0.0.25",
		"autoprefixer": "^10.4.16",
		"marked": "^9.1.4",
		"pocketbase": "^0.19.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"svelte-french-toast": "^1.2.0",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.4.1",
		"typescript": "^5.2.2",
		"vite": "^4.4.2"
	},
	"type": "module"
}
