{
  "name": "@valiantlynx/react-ui",
  "version": "0.0.1",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/index.css"
  },
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valiantlynx/react-ui.git"
  },
  "bugs": {
    "url": "https://github.com/valiantlynx/react-ui/issues"
  },
  "homepage": "https://github.com/valiantlynx/react-ui#readme",
  "scripts": {
    "build": "tsup",
    "lint": "eslint src/",
    "dev": "tsup --watch",
    "check-types": "tsc --noEmit",
    "changeset": "changeset",
    "publish:packages": "changeset publish",
    "version:packages": "changeset version",
    "release": "yarn build && yarn version:packages && yarn publish:packages"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/react": "^18.2.5",
    "@valiantlynx/eslint-config-custom": "0.0.1",
    "@valiantlynx/tailwind-config": "0.0.9",
    "@valiantlynx/tsconfig": "^0.0.25",
    "autoprefixer": "^10.4.13",
    "postcss": "^8.4.20",
    "react": "^18.2.0",
    "tsup": "^7.2.0",
    "typescript": "^5.1.6"
  }
}
